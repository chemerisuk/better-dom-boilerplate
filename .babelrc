{
    "presets": [
        ["@babel/env", {
            "targets": {
                browsers: ["ChromeAndroid 30", "iOS 7", "IE 10"]
            },
            "loose": true,
            "modules": false
        }]
    ],
    "plugins": [
        "html-tag",
        "iife-wrap",
        ["transform-css-import-to-string", {
            "browsers": ["ChromeAndroid 30", "iOS 7", "IE 10"],
            "features": {
                "nesting-rules": true,
                "custom-properties": {"preserve": false},
                "custom-media-queries": {"preserve": false}
            }
        }]
    ]
}
